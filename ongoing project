# Addition of 2 numbers
def add(h, j):
    return h + j

# Subtraction of two numbers
def subtract(h, j):
    return h - j

# Multiplication of two numbers
def multiply(h, j):
    return h * j

# Division of two numbers
def divide(h, j):
    return h / j


print("Select operation.")
print("1.Add")
print("2.Subtract")
print("3.Multiply")
print("4.Divide")

