# Addition of 2 numbers
def add(x, y):
    return x + y

# Subtraction of two numbers
def subtract(x, y):
    return x - y

# Multiplication of two numbers
def multiply(x, y):
    return x * y

# Division of two numbers
def divide(x, y):
    return x / y


print("Select operation.")
print("1.Add")
print("2.Subtract")
print("3.Multiply")
print("4.Divide")

